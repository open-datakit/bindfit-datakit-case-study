{
  "name": "fitGraphHighcharts",
  "title": "Fit curve",
  "description": "Optimised binding curve plotted against data and fit residuals",
  "spec": {
    "dataPool": {
      "connectors": [
        {
          "id": "connector",
          "type": "JSON",
          "options": {
            "data": {
              "resources": [
                "data",
                "fit",
                "residuals"
              ]
            },
            "dataModifier": {
              "type": "Math",
              "columnFormulas": [
                {
                  "column": "x",
                  "formula": "B1/A1"
                }
              ]
            }
          }
        }
      ]
    },
    "gui": {
      "layouts": [
        {
          "id": "layout-1",
          "rows": [
            {
              "cells": [
                {
                  "id": "cell-1",
                  "height": "300px"
                }
              ]
            },
            {
              "cells": [
                {
                  "id": "cell-2",
                  "height": "200px"
                }
              ]
            }
          ]
        }
      ]
    },
    "components": [
      {
        "cell": "cell-1",
        "type": "Highcharts",
        "sync": {
          "highlight": true
        },
        "connector": [
          {
            "id": "connector",
            "columnAssignment": [
              {
                "seriesId": "data",
                "data": [
                  "x",
                  [
                    "data_y0",
                    "data_y1",
                    "data_y2"
                  ]
                ]
              }
            ]
          },
          {
            "id": "connector",
            "columnAssignment": [
              {
                "seriesId": "fit",
                "data": [
                  "x",
                  [
                    "fit_y0",
                    "fit_y1",
                    "fit_y2"
                  ]
                ]
              }
            ]
          }
        ],
        "chartOptions": {
          "colors": [
            "#79BCB8",
            "#0B4F6C",
            "#197BBD",
            "#033860",
            "#47A8BD",
            "#1E3888",
            "#EE6C4D",
            "#FA8334"
          ],
          "chart": {
            "marginRight": 100,
            "marginLeft": 100
          },
          "title": {
            "text": null
          },
          "xAxis": {
            "title": {
              "text": "Equivalent total [G]/[H]"
            },
            "gridLineWidth": 1
          },
          "yAxis": {
            "title": {
              "text": "Delta (ppm)"
            }
          },
          "legend": {
            "align": "right",
            "verticalAlign": "middle",
            "layout": "vertical"
          },
          "tooltip": {
            "crosshairs": [
              true,
              false
            ],
            "shared": true,
            "valueDecimals": 4,
            "valueSuffix": " ppm"
          },
          "series": [
            {
              "id": "data",
              "name": "Data",
              "type": "line",
              "lineWidth": 0,
              "marker": {
                "enabled": true
              }
            },
            {
              "id": "fit",
              "name": "Fit",
              "type": "line",
              "marker": {
                "enabled": false,
                "states": {
                  "hover": {
                    "enabled": false
                  }
                }
              }
            }
          ]
        }
      },
      {
        "cell": "cell-2",
        "type": "Highcharts",
        "sync": {
          "highlight": true
        },
        "connector": [
          {
            "id": "connector",
            "columnAssignment": [
              {
                "seriesId": "residuals",
                "data": [
                  "x",
                  [
                    "residuals_y0",
                    "residuals_y1",
                    "residuals_y2"
                  ]
                ]
              }
            ]
          }
        ],
        "chartOptions": {
          "colors": [
            "#79BCB8",
            "#0B4F6C",
            "#197BBD",
            "#033860",
            "#47A8BD",
            "#1E3888",
            "#EE6C4D",
            "#FA8334"
          ],
          "chart": {
            "marginRight": 100,
            "marginLeft": 100
          },
          "title": {
            "text": null
          },
          "xAxis": {
            "title": {
              "text": "Equivalent total [G]/[H]"
            },
            "gridLineWidth": 1
          },
          "yAxis": {
            "title": {
              "text": "Residuals"
            }
          },
          "legend": {
            "align": "right",
            "verticalAlign": "middle",
            "layout": "vertical"
          },
          "tooltip": {
            "crosshairs": [
              true,
              false
            ],
            "shared": true,
            "valueDecimals": 4,
            "valueSuffix": " ppm"
          },
          "series": [
            {
              "id": "residuals",
              "name": "Data",
              "type": "line",
              "marker": {
                "enabled": false,
                "states": {
                  "hover": {
                    "enabled": true
                  }
                }
              }
            }
          ]
        }
      }
    ]
  },
  "specType": "highcharts",
  "resources": [
    "data",
    "fit",
    "residuals"
  ]
}
